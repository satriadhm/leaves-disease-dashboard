{
  "name": "plant-disease-detection-dashboard",
  "version": "2.0.0",
  "description": "Modern Plant Disease Detection Dashboard with AI Integration",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:prod": "NODE_ENV=production webpack --mode=production",
    "build:analyze": "NODE_ENV=production webpack --mode=production --env analyze",
    "dev": "webpack serve --mode=development",
    "start": "webpack serve --mode=development",
    "preview": "webpack serve --mode=production",
    "lint": "eslint src --ext .js --max-warnings 0",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,css}\"",
    "clean": "rimraf dist",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "test:coverage": "echo \"No tests specified\" && exit 0",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "keywords": [
    "plant-disease",
    "ai",
    "machine-learning",
    "agriculture",
    "dashboard",
    "javascript",
    "webpack",
    "vercel"
  ],
  "author": "Plant Disease Detection Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.4",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "core-js": "^3.42.0",
    "regenerator-runtime": "^0.14.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satriadhm/leaves-disease-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/satriadhm/leaves-disease-dashboard/issues"
  },
  "homepage": "https://github.com/satriadhm/leaves-disease-dashboard#readme",
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ]
  }
}
