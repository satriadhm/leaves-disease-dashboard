{
  "name": "plant-disease-detection-dashboard",
  "version": "2.0.0",
  "description": "Modern Plant Disease Detection Dashboard with AI Integration",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:prod": "NODE_ENV=production webpack --mode=production --stats=normal",
    "build:analyze": "NODE_ENV=production webpack --mode=production --analyze",
    "dev": "webpack serve --mode=development --open",
    "start": "webpack serve --mode=development --open",
    "preview": "webpack serve --mode=production",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.{js,css}",
    "format:check": "prettier --check src/**/*.{js,css}",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "keywords": [
    "plant-disease",
    "ai",
    "machine-learning",
    "agriculture",
    "dashboard",
    "javascript",
    "webpack",
    "vercel"
  ],
  "author": "Plant Disease Detection Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "core-js": "^3.33.0",
    "regenerator-runtime": "^0.14.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satriadhm/leaves-disease-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/satriadhm/leaves-disease-dashboard/issues"
  },
  "homepage": "https://github.com/satriadhm/leaves-disease-dashboard#readme",
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/tests/setupTests.js"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@styles/(.*)$": "<rootDir>/src/styles/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/main.js",
      "!src/tests/**/*"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}